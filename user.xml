  <template name="bdcuslist" value="private static class CustomListAdapter extends android.widget.BaseAdapter {&#10;        private java.util.ArrayList&lt;com.example.lin.android3.MainList.NewsItem&gt; listData;&#10;        private android.view.LayoutInflater layoutInflater;&#10;        public CustomListAdapter(android.content.Context context, java.util.ArrayList&lt;com.example.lin.android3.MainList.NewsItem&gt; listData) {&#10;            this.listData = listData;&#10;            layoutInflater = android.view.LayoutInflater.from(context);&#10;&#10;        }&#10;        @java.lang.Override&#10;        public int getCount() {&#10;            return listData.size();&#10;        }&#10;        @java.lang.Override&#10;        public java.lang.Object getItem(int position) {&#10;            return listData.get(position);&#10;        }&#10;&#10;        @java.lang.Override&#10;        public long getItemId(int position) {&#10;            return position;&#10;        }&#10;        public android.view.View getView(int position, android.view.View convertView, android.view.ViewGroup parent) {&#10;            com.example.lin.android3.MainList.CustomListAdapter.ViewHolder holder;&#10;            if (convertView == null) {&#10;                convertView = layoutInflater.inflate(com.example.lin.android3.R.layout.custlist, null);&#10;                holder = new com.example.lin.android3.MainList.CustomListAdapter.ViewHolder();&#10;                holder.TitleView = (android.widget.TextView) convertView.findViewById(com.example.lin.android3.R.id.Title1);&#10;                holder.AddrView = (android.widget.TextView) convertView.findViewById(com.example.lin.android3.R.id.addr);&#10;                convertView.setTag(holder);&#10;            } else {&#10;                holder = (com.example.lin.android3.MainList.CustomListAdapter.ViewHolder) convertView.getTag();&#10;            }&#10;&#10;            holder.TitleView.setText(listData.get(position).getTitle());&#10;            holder.AddrView.setText(&quot;By, &quot; + listData.get(position).getAddress());&#10;&#10;            return convertView;&#10;        }&#10;        static class ViewHolder {&#10;            android.widget.TextView TitleView;&#10;            android.widget.TextView AddrView;&#10;        }&#10;&#10;    }&#10;&#10; private static class NewsItem {&#10;        private java.lang.String Title;&#10;        private java.lang.String Address;&#10;        public java.lang.String getTitle(){&#10;            return Title;&#10;        }&#10;        public java.lang.String getAddress(){&#10;            return Address;&#10;        }&#10;&#10;        public void setTitle(java.lang.String title  )&#10;        {&#10;            this.Title=title;&#10;        }&#10;        public void setAddress(java.lang.String address)&#10;        {&#10;            this.Address=address;&#10;        }&#10;        @java.lang.Override&#10;        public java.lang.String toString(){&#10;            return Title+&quot;:&quot;+Address;&#10;        }&#10;    }" description="CustomList" toReformat="true" toShortenFQNames="true">
